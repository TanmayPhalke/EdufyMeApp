<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Back To Dashboard"
             x:Class="App.uStaff.NotesCreate">
    <ContentPage.Content>
        <StackLayout Padding="0">
            <WebView x:Name="MyBrowser" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
            <Button x:Name="ModifyNotes" Text="Delete Notes" TextColor="White" BackgroundColor="DodgerBlue" Clicked="ModifyNotes_Clicked" Margin="0,-5,0,0" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>